extends layout

block content
.w3-container
    if locals.user
      h4 You are logged in!
    else
      h4 You are not logged in! Please #[a(href="/login") Log In] to continue.
  h1= title
  
  ul
    each artist in artist_list
      li 
        a(href=artist.url) #{artist.name} 
        

    else
      li There are no artists.